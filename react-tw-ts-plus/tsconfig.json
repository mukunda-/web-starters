{
   "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": false,
      "module": "ESNext",
      "target": "ES6",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "baseUrl": "./",
      "paths": {
         "@/*": ["./src/*"]
      },
      "sourceMap": true
   },
   "include": ["src/**/*"],
   "exclude": ["node_modules"]
}
